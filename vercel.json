{
    "version": 2,
    "builds": [
        {
            "src": "/src/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "/src/models.js",
            "use": "@vercel/node"
        },
        {
            "src": "/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/chat",
            "dest": "/src/app.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/src/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}